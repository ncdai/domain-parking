{
  "name": "sell-domains",
  "version": "3.4.4",
  "main": "index.js",
  "repository": "git@github.com:ncdai/sell-domains.git",
  "author": "Nguyen Chanh Dai <dai@chanhdai.com>",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development nodemon index.js",
    "start": "node index",
    "tailwind:watch": "tailwindcss -i ./styles/app.css -o ./static/app.css --watch",
    "tailwind:build": "tailwindcss -i ./styles/app.css -o ./static/app.css --minify",
    "pm2:start": "pm2 start ./ecosystem.config.js --env production ; pm2 status",
    "pm2:reload": "pm2 reload ./ecosystem.config.js --env production ; pm2 status",
    "pm2:stop": "pm2 stop ./ecosystem.config.js ; pm2 status",
    "pm2:delete": "pm2 delete ./ecosystem.config.js ; pm2 status"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "nodemon": "^3.0.1",
    "standard": "^16.0.4",
    "tailwindcss": "^3.4.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "global": []
  }
}
